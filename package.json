{
  "name": "mapp-fashion-react-native-sdk-expo",
  "author": "Mapp Digital <productmanagement@mapp.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mapp-digital/Mapp-Fashion-ReactNative-Plugin-Expo.git"
  },
  "homepage": "https://github.com/mapp-digital/Mapp-Fashion-ReactNative-Plugin-Expo#readme",
  "version": "0.1.1",
  "description": "Expo Storage SDK for Dressipi integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts *.md *.json",
    "format:check": "prettier --check src/**/*.ts *.md *.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "sdk",
    "react-native",
    "expo"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "expo-secure-store": "^14.2.3"
  }
}
